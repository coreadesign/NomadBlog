<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <style>
        table{width:100%;table-layout: fixed;
            border-collapse:collapse;}
        td{padding:10px;
            border: 1px solid #000;}
    </style>
</head>
<body>
<form id="joinForm" action="/write" method="post">
<table>
    <%
        userInfo.forEach(function(user){
    %>
    <tr>
        <td></td>
        <td>
            <%=user.ids%>
        </td>
        <td>
            <%=user.mail%>
        </td>
        <td>
            <%=user.passwd%>
        </td>
    </tr>
    <%
        });
    %>
</table>
    <div>
        <a href="/write">글쓰기</a>
    </div>
</form>
</body>
</html>